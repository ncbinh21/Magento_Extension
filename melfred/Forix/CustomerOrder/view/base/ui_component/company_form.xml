<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="general" sortOrder="10">
        <field name="distributor_zipcode" formElement="textarea" sortOrder="100">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">distributor_zipcode</item>
                    <item name="disabled" xsi:type="boolean">true</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true"><![CDATA[Distributor Zipcode (Manage orders by zipcode)
                    Seperated by line-break or ,]]></label>
            </settings>
        </field>
    </fieldset>
</form>
