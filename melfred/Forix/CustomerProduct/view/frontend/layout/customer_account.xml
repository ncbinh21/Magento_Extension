<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Customer\Block\Account\SortLinkInterface" name="customer-account-product-orders-link">
                <arguments>
                    <argument name="path" xsi:type="string">customer/product</argument>
                    <argument name="label" xsi:type="string" translate="true">My Products</argument>
                    <argument name="sortOrder" xsi:type="number">235</argument>
                </arguments>
            </block>
            <block class="Magento\RequisitionList\Block\Catalog\Product\ProductList\Item\AddTo\Requisition" name="add.require.list.my.product" template="Magento_RequisitionList::checkout/requisition.phtml" />
        </referenceBlock>
    </body>
</page>
