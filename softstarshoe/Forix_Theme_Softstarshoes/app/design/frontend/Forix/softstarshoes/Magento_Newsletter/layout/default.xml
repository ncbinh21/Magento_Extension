<page layout="1column" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="footer_inner">
            <block class="Magento\Cms\Block\Block" name="footer.socials" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">footer_socials</argument>
                </arguments>
            </block>
        </referenceContainer>
        <move element="form.subscribe" destination="footer_inner" before="-"/>
    </body>
</page>
